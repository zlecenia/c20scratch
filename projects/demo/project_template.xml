<xml xmlns="https://developers.google.com/blockly/xml">
  <variables></variables>
  <block type="add_view" id="main_view" x="100" y="100">
    <field name="NAME">Project Template</field>
    <next>
      <block type="ui_heading" id="title">
        <field name="LEVEL">1</field>
        <field name="TEXT">🏗️ Project Title</field>
        <next>
          <block type="ui_paragraph" id="description">
            <field name="TEXT">Brief description of what this project does.</field>
            <next>
              <block type="ui_divider" id="section_break">
                <next>
                  <block type="ui_heading" id="section_title">
                    <field name="LEVEL">2</field>
                    <field name="TEXT">📋 Main Section</field>
                    <next>
                      <block type="ui_container" id="main_content">
                        <next>
                          <block type="ui_paragraph" id="content_desc">
                            <field name="TEXT">Add your content blocks here...</field>
                            <next>
                              <block type="ui_alert" id="tips">
                                <field name="TYPE">info</field>
                                <field name="TEXT">💡 Tips: Use this template as a starting point for new projects. Replace placeholders with your content.</field>
                              </block>
                            </next>
                          </block>
                        </next>
                      </block>
                    </next>
                  </block>
                </next>
              </block>
            </next>
          </block>
        </next>
      </block>
    </next>
  </block>
</xml>
